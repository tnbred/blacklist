<div class="section row">
<div class="articles col-md-offset-3 col-md-6"> 
  {{#each articles}}
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">{{title}}<small class="pull-right">by {{userName}} {{formattedCreatedAt}}</small></h3>
  </div>
  <div class="panel-body">
    {{content}}
  </div>
</div>
{{/each}}
</div>
</div>