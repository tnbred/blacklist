<div class="panel panel-primary">
  <!-- Default panel contents -->
  <div class="panel-heading">{{results.listName}}</div>
  <div class="panel-body">
    <p>You are ranked {{results.currentUser_Rank}} out of {{results.totalPeople}} people and you have {{results.currentUser_Points}} votes left. </p>
  </div>
<table class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>Rank</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Points</th>
    <th></th>
  </tr>
  </thead>
  <tbody id="list_table" list_id={{results.listId}} > 
      {{#each results.users}}
      <tr id="list_item_0" points={{userPoints}}  user_to_id={{user._id}}>
        <td class="item_rank">{{userRank}}</td>
        <td class="item_firstname">{{user.firstName}}</td>
        <td class="item_lastname">{{user.lastName}}</td>
        <td class="item_points">{{userPoints}}</td>
        <td>
          <div class="hidden add_point_div">
            <select>
                  <option value="0">0</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
                  <option value="6">6</option>
                  <option value="7">7</option>
                  <option value="8">8</option>
                  <option value="9">9</option>
                  <option value="10">10</option>
                  <option value="11">11</option>
                  <option value="12">12</option>
                  <option value="13">13</option>
                  <option value="14">14</option>
                  <option value="15">15</option>
                  <option value="16">16</option>
                  <option value="17">17</option>
                  <option value="18">18</option>
                  <option value="19">19</option>
                  <option value="20">20</option>
                  <option value="21">21</option>
                  <option value="22">22</option>
                  <option value="23">23</option>
                  <option value="24">24</option>
                  <option value="25">25</option>
            </select>
            <button class="btn btn-danger valid_vote">Vote!</button>
          </div>
          <div class="add_point_button">
            <button class="btn">+</button>
            <div class="alert message fade in">

            </div>
          </div>
        </td>
      </tr>
      {{/each}}
  </tbody>
</table>

</div>